<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="2" id="org.theadso.adso" ios-CFBundleVersion="2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>ADSO</name>
    <description>Association of Dental Support Organizations</description>
    <author email="info@theadso.org" href="https://www.theadso.org">ADSO Administrator</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="https://www.pathlms.com/*" />
    <allow-navigation href="https://www.theadso.org/*" />
    <allow-navigation href="about*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^7.0.0" />
    <engine name="ios" spec="^4.5.4" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
